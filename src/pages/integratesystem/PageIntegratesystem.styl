.t-tab-head
    position fixed
    z-index 10
.t-self
    font-size 30px
    font-weight bold
.t-tab-bar-items
    z-index 50
.t-group-list-item:after
    border-bottom 0
.table>thead>tr>th
    border-bottom 0
.table>tbody>tr>td
    vertical-align middle
    font-size 14px
.t-all
    width 100%
.table>tbody>tr>td.t-self
    font-size 22px
    font-weight bold
.t-all li
    float left
    font-weight bold
    line-height 38px
.t-userAvatar
    box-shadow 0 0 2px 3px #48B4FF inset
.t-header
    width 100%
    height 38px
    background :#F5F5F9
    border-bottom 1px solid #ddd
.rank
    width 39%
    padding-left 12px
.name
    width 41%
.t-honor
    width 20%
.integratesystem .t-dynamic
    background-image url('./dynamic.png')
.t-loading
    text-align center
    line-height 17px
    display none
.t-lh
    line-height 32px
@-webkit-keyframes spin {
    from {
        -webkit-transform: rotate(0deg);
    }
    to {
        -webkit-transform: rotate(360deg);
    }
}

.glyphicon-refresh-animate {
    -animation: spin .7s infinite linear;
    -webkit-animation: spin .7s infinite linear;
}
.t-group-list-item
    width 100%
    height 100%
    position relative
.integratesystem .t-list-delete-btn
    background none
.integratesystem .t-score
    font-weight 700
.integratesystem .t-Score
    margin-right 5.36%
    margin-top 2%
.integratesystem .t-success
    color #F5A623
.integratesystem .t-danger
    color #9B9B9B
.integratesystem .t-grantTime
    color #9B9B9B
