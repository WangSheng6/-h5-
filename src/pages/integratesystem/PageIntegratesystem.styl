.integratesystem
    width 100%
    height 100%
    display block
.detail_list
    margin-top 45px
.score_info
    height 0.9733rem
    color:#868686
.t-selfAvatar
    border 2px solid #48B4FF
.t-group-list-item
    border-bottom: 2px solid #eee
.t-list-wrap
    padding 5px 0
.t-list-title
    line-height 14px
.t-list-text
    line-height 16px
.honor,.available
    margin 0
.t-title
    font-size 16px
    font-weight bold
    padding-top 6px
p
    margin 0 0 3px
.t-date
    font-size 12px
    color: #999
    float right
    margin-right 10px
.t-text
    font-size 13px
    margin 0 0 6px
    color: #8a8a8a
